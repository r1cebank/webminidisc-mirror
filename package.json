    "netmd-exploits": "^0.5.6",
    "netmd-js": "^4.1.8",
    "netmd-tocmanip": "^0.1.5",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-redux": "^9.1.0",
    "react95": "^4.0.0",
    "recorderjs": "github:asivery/Recorderjs",
    "redux": "^5.0.1",
    "redux-batched-actions": "^0.5.0",
    "shazam-api": "^0.2.0",
    "styled-components": "^6.1.8",
