    "@mui/lab": "^5.0.0-alpha.161",
    "@mui/material": "^5.15.5",
    "@reduxjs/toolkit": "^2.2.3",
    "async-mutex": "^0.5.0",
    "buffer": "^6.0.3",
    "chonky": "^2.3.2",
